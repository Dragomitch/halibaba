package marche_halibaba_clients;

import java.sql.Date;

public class EstimateRequest {
	private int id;
	private String description;
	private Address constructionAddress;
	private Address invoicingAddress;
	private Date deadline;
	private Date pubDate;
	private Estimate[] submittedEstimates;
	private Client client;
	
	public EstimateRequest(int id, String description, Address constructionAddress, Address invoicingAddress,
			Date deadline, Date pubDate, Client client) {
		
	}
	
	public EstimateRequest(int id, String description, Date deadline, Date pubDate, Client client) {
		this(id, description, null, null, deadline, pubDate)
	}
	
}
